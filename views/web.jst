{{
    function isYoutube(url){
        var regExp = /^.*((youtu.be\/)|(v\/)|(\/u\/\w\/)|(embed\/)|(watch\?))\??v?=?([^#\&\?]*).*/;
        var match = url.match(regExp);
        return (match&&match[7].length==11)? match[7] : false;
    }
}}


{{
    function urlDecode(url){
        try {
            return decodeURIComponent(url);
        } catch(e) {
            return url;
        }
    }
}}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{{=it.q + " - " + it.__("SEARCH")}}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="search" type="application/opensearchdescription+xml" href="/opensearch.xml" title="">
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta content="always" name="referrer">
    <link rel="stylesheet" type="text/css" href="css/styler.css">
    <script type="text/javascript" src="/js/jquery-2.0.2.min.js"></script>
    <script>
        $(function(){
            var u = $("#suggest-align"),
                a = $(".ac_wrap"),
                m = $(".ac_menu"),
                f = $("#head form"),
                i = $("#keyword"),
                x = null,
                s = "";
                w = "";
                ai = -1;

            f.on("submit", function() {
                if ($.trim(i.val()) == "") return i.focus(), !1
            });
            
            m.on("mouseover", "li", function() {
                $(this).addClass("selected");
                s = $(this).attr("acvalue");
                i.val($(this).attr("acvalue"));
            });
            
            m.on("mouseout", "li", function() {
                $(this).removeClass("selected");
                i.val(w);
                s = "";
            });
            
            m.on("click", "li", function(){
                f.submit();
                a.hide();
            });

            i.on({
                focus: function() {
                    u.addClass("focus");
                    if ($.trim(i.val()) != "" && a.find("li").length) { 
                        a.show();
                    }
                },
                blur: function() {
                    u.removeClass("focus");
                    if (s == "") {
                        a.hide();
                    }
                }, 
                keydown: function(e) {
                    var t = e.keyCode || e.which;
                    if (t === 13) {
                        
                    } else if (t === 38) {
                        if (a.find("li").length == 0) return;
                        if (ai == -1) return;
                        
                        var el = $("li[ac_index='" + ai + "']");
                        el.removeClass("selected");

                        ai--;
                        
                        var el = $("li[ac_index='" + ai + "']");
                        el.addClass("selected");
                        s = el.attr("acvalue");
                        i.val(el.attr("acvalue"));
                        e.preventDefault();
                        return false;
                        
                    } else if (t === 40) {
                        if (a.find("li").length == 0) return;
                        if (ai == a.find("li").length - 1) return;
                        
                        var el = $("li[ac_index='" + ai + "']");
                        el.removeClass("selected");

                        ai++;
                        
                        var el = $("li[ac_index='" + ai + "']");
                        el.addClass("selected");
                        s = el.attr("acvalue");
                        i.val(el.attr("acvalue"));
                        e.preventDefault();
                        return false;
                    }
                },
                input: function() {
                    w = $.trim($(this).val());
                    if (x != null) x.abort();
                    if (w != "") {
                        x = $.ajax({
                            type: "GET",
                            url: "/autocomplete?q=" + encodeURIComponent(w) + "&e={{=it.user.e}}",
                            success: function(r) {
                                m.empty();
                                var ir = 0;
                                r.forEach(function(re){
                                    m.append('<li ac_index="' + ir + '" acvalue="' + re + '">' + re + '</li>');
                                    ir++;
                                });
                                if (ir > 0) { 
                                    a.show();
                                }
                            }
                        });
                    } else {
                        s = "";
                        m.empty();
                        a.hide();
                    }
                }
            });
        });
    </script>
</head>
<body>
